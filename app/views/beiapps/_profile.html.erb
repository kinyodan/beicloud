          <div class="card h-100">
            <div class="card-header pb-0 p-3">
              <div class="row">
                <div class="col-md-8 d-flex align-items-center">
                  <h6 class="mb-0">App Information</h6>
                </div>
                <div class="col-md-4 text-end">
                  <a href="javascript:;">
                    <i class="fas fa-user-edit text-secondary text-sm" data-bs-toggle="tooltip" data-bs-placement="top" title="Edit Profile"></i>
                  </a>
                </div>
              </div>
            </div>
            <div class="card-body p-3">
              <p class="text-sm">
              <%= beiapp.summary%>
              </p>
              <hr class="horizontal gray-light my-4">
              <ul class="list-group">
                <li class="list-group-item border-0 ps-0 pt-0 text-sm"><strong class="text-dark">Full Name:</strong> &nbsp; <%= beiapp.owner_name %></li>
                <li class="list-group-item border-0 ps-0 text-sm"><strong class="text-dark">Mobile:</strong> &nbsp; <%= beiapp.owner_contact_phone %></li>
                <li class="list-group-item border-0 ps-0 text-sm"><strong class="text-dark">Email:</strong> &nbsp; <%= beiapp.owner_contact_email %></li>
                <li class="list-group-item border-0 ps-0 text-sm"><strong class="text-dark">Location:</strong> &nbsp; <%= beiapp.owner_location %></li>
                <li class="list-group-item border-0 ps-0 pb-0">
                  <strong class="text-dark text-sm">Social:</strong> &nbsp;
                  <a class="btn btn-instagram btn-simple mb-0 ps-1 pe-2 py-0" href="<%= beiapp.owner_social_instagram %>">
                    <i class="fa-brands fa-github fa-2x"></i>
                  </a>
                  <a class="btn btn-facebook btn-simple mb-0 ps-1 pe-2 py-0" href="<%= beiapp.owner_social_twitter %>">
                    <i class="fab fa-facebook fa-2x"></i>
                  </a>
                  <a class="btn btn-twitter btn-simple mb-0 ps-1 pe-2 py-0" href="<%= beiapp.owner_social_twitter %>">
                    <i class="fab fa-twitter fa-2x"></i>
                  </a>
                  <a class="btn btn-instagram btn-simple mb-0 ps-1 pe-2 py-0" href="<%= beiapp.owner_social_instagram %>">
                    <i class="fab fa-instagram fa-2x"></i>
                  </a>
                </li>
              </ul>
            </div>
          </div>
