<div class="container">
  <div class="row">
    <div class="Absolute-Center is-Responsive">
    <a class="header-logo" href="https://github.com/" aria-label="Homepage" data-ga-click="(Logged out) Header, go to homepage, icon:logo-wordmark">
      <svg height="48" aria-hidden="true" viewBox="0 0 16 16" version="1.1" width="48" data-view-component="true" class="octicon octicon-mark-github">
    <path fill-rule="evenodd" d="M8 0C3.58 0 0 3.58 0 8c0 3.54 2.29 6.53 5.47 7.59.4.07.55-.17.55-.38 0-.19-.01-.82-.01-1.49-2.01.37-2.53-.49-2.69-.94-.09-.23-.48-.94-.82-1.13-.28-.15-.68-.52-.01-.53.63-.01 1.08.58 1.23.82.72 1.21 1.87.87 2.33.66.07-.52.28-.87.51-1.07-1.78-.2-3.64-.89-3.64-3.95 0-.87.31-1.59.82-2.15-.08-.2-.36-1.02.08-2.12 0 0 .67-.21 2.2.82.64-.18 1.32-.27 2-.27.68 0 1.36.09 2 .27 1.53-1.04 2.2-.82 2.2-.82.44 1.1.16 1.92.08 2.12.51.56.82 1.27.82 2.15 0 3.07-1.87 3.75-3.65 3.95.29.25.54.73.54 1.48 0 1.07-.01 1.93-.01 2.2 0 .21.15.46.55.38A8.013 8.013 0 0016 8c0-4.42-3.58-8-8-8z"></path>
</svg>
    </a>
      <div id="logo-container"> <h3>login to github</h3></div>
      <div class="col-sm-12 col-md-offset-1">
        <%= form_for(resource, as: resource_name, url: user_github_omniauth_authorize_path, html:{id:"loginForm"}) do |f| %>
          <%# <form action="" id="loginForm"> %>
            <div class="form-group input-group">
              <span class="input-group-addon"><img src="https://res.cloudinary.com/selae-learning/image/upload/v1674759762/beicloud-assets/icons8-user-30_ehvqb4.png"/> </span>
              <%= f.email_field :email, autofocus: true, autocomplete: "email" , placeholder: "Email ", class:"form-control" %>
              <%# <input class="form-control" type="text" name='username' placeholder="username"/>           %>
            </div>
            <div class="form-group input-group">
              <span class="input-group-addon"><img src="https://res.cloudinary.com/selae-learning/image/upload/v1674759955/beicloud-assets/icons8-private-lock-30_rstghb.png" /></span>
              <%= f.password_field :password, autocomplete: "current-password", placeholder: "Password" ,class:"form-control" %>

              <%# <input class="form-control" type="password" name='password' placeholder="password"/>      %>
            </div>
            <div class="checkbox">
              <label>
                <input type="checkbox"> I agree to the <a href="#">Terms and Conditions</a>
              </label>
            </div>
            <div class="form-group">
              <%= f.button "Log in", class:"btn btn-def btn-block" %>

              <%# <button type="button" class="btn btn-def btn-block" >Login</button> %>
            </div>
            <div class="form-group text-center">
              <a href="#">Forgot Password</a>&nbsp;|&nbsp;<a href="#">Support</a>
            </div>
          <%# </form>  %>
        <% end %>   
    
      </div>  
    </div>    
  </div>
</div>

